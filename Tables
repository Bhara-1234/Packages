CREATE TABLE InsurancePackages (
    insp_id serial PRIMARY KEY,
    insp_title varchar(60),
    insp_desc varchar(255),
    insp_status char(2),
    insp_range_start numeric,
    insp_range_end numeric,
    insp_agelimit_start int,
    insp_agelimit_end int
);
