select * from InsurancePackages where  insp_range_start <28 and insp_range_end>28;
create table InsurancePackages

SELECT *
FROM InsurancePackages
WHERE 28 BETWEEN insp_agelimit_start AND insp_agelimit_end;
select * FROM InsurancePackages
select * FROM InsurancePackages where insp_status='ALL' and 28 BETWEEN insp_agelimit_start AND insp_agelimit_end;
